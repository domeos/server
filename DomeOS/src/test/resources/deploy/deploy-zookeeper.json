{
  "deployName":"zookeeper",
  "creator":{
    "creatorType":"USER",
    "creatorId":1
  },
  "containerDrafts":[
    {
      "image":"zookeeper-stateful",
      "registry":"0.0.0.0:5000",
      "tag":"0.4",
      "cpu":2.0,
      "mem":1024
    }
  ],
  "replicas":3,
  "hostEnv":"PROD",
  "clusterName":"mycluster",
  "stateful":true,
  "scalable":false,
  "volumes":["/opt/zookeeper/data", "/opt/zookeeper/conf"],
  "hostList":["test-host"],
  "loadBalanceDraft":[
    {
      "name": "client",
      "port": 18780,
      "targetPort":2181,
      "type":"TCP",
      "clusterName":"mycluster"
    },
    {
      "name": "followers",
      "port": 18781,
      "targetPort":2888,
      "type":"TCP",
      "clusterName":"mycluster"
    },
    {
      "name": "election",
      "port": 18782,
      "targetPort":3888,
      "type":"TCP",
      "clusterName":"mycluster"
    }
  ]
}
